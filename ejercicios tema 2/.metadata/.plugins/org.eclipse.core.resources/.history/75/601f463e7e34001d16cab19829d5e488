package tiposDeDatos;

public class CaracteresyBoolean {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		char c1='r';
		char d1="8";
		String cadena="Hola que tal estas?";
		String dni="39456069y";
		String telefono="675486821";
		telefono.charAt(d1)
		// uso string en vez de long xq no los voy a sumar o hacer operaciones
		//String es una variable compleja, las complejas empiezan por mayusculas
		//las primitivas empiezan con minuscula, solo pueden almacenar valores
		//las complejas pueden almacenar valores y ejecutar funciones
		//por cada variable primitiva hay una compleja que empieza por mayus
		
		
		//Valores true o false
		
		boolean condicion=true;
		condicion = false;

	}

}
